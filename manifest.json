{
  "manifest_version": 2,

  "name": "gbooksdldr",
  "description": "Chrome extension to download books on Google Books",
  "version": "1.0",

  "background": {
    	"scripts" : ["./js/background.js", "./js/jspdf.js"],
	"persistent" : false
  },

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ]
}
